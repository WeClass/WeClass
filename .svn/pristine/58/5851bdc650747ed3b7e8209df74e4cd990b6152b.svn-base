<!doctype html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
		<title>侧栏</title>
		<link rel="stylesheet" type="text/css" href="../css/api.css"/>
		<link rel="stylesheet" type="text/css" href="../css/sidepage/sidepage.css" />
	</head>
	<body>
		<!--个人信息-->
		<div class="outimgbackground"></div>
		<div class="imgbackground">
		<div class="login"  onclick="openNewWin('mycenter')" id="topbar"></div>
		<div id="logintitle" class="logintitle" onclick="openNewWin('mycenter')">
		<span id="loginuser"class="loginuser">登录</span>
		</div>
		</div>
		<!--外面的大盒子-->
		<div class="outInformation">
		<!--我的视频-->
		<div class="item" tapmode="itemhover" onclick="openNewWin('video/video_win')"  >
			<div class="itemlogo"><img src="../image/sidepage/side_myvideo.png" alt="">
			</div>
			<div class="itemshelf">
				<div class="shelfinfo01">
					视频
				</div>
			</div>
		</div>
		<!--我的PPT-->
		<div class="item" tapmode="itemhover" onclick="openNewWin('ppt/ppt_win')"  >
			<div class="itemlogo"><img src="../image/sidepage/side_myppt.png" alt="">
			</div>
			<div class="itemshelf">
				<div class="shelfinfo01">
					PPT
				</div>
			</div>
		</div>
		<!--我的讲义-->
		<div class="item" tapmode="itemhover" onclick="openNewWin('mylecture/lecture_win')"  >
			<div class="itemlogo"><img src="../image/sidepage/side_mylecture.png" alt="">
			</div>
			<div class="itemshelf">
				<div class="shelfinfo01">
					讲义
				</div>
			</div>
		</div>
		<!--我的作业-->
		<div class="item" tapmode="itemhover" onclick="openNewWin('myhomework/homework_win')"  >
			<div class="itemlogo"><img src="../image/sidepage/side_myhomework.png" alt="">
			</div>
			<div class="itemshelf">
				<div class="shelfinfo01">
					作业
				</div>
			</div>
		</div>
		<!--我的疑问-->
		<div class="item" tapmode="itemhover" onclick="openNewWin('myquestion/feedback_win')"  >
			<div class="itemlogo"><img src="../image/sidepage/side_myquetion.png" alt="">
			</div>
			<div class="itemshelf">
				<div class="shelfinfo01">
					答疑
				</div>
			</div>
		</div>
		</div>
		<!--外面的大盒子-->
	</body>
	<script type="text/javascript" src="../script/api.js"></script>
	<script type="text/javascript" src="../script/sidepage/sidepage.js"></script>
	
</html>















