body {
	background-color: #fff;
}
/*header*/
header {
	background-color: #10B6F2;
	
}
.header {
	display: -webkit-box;
	display: -webkit-flex;
	
}
.header a {
	display: block;
	padding: 20px;
}
.title {
    color: #FFFFFF;
	line-height: 40px;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	text-align: center;
}
.header .menu {
	background-size: 20px;
	background-repeat: no-repeat;
	margin-bottom: 5px;
	margin-left: 10px;
	background-position: center;
	background-image: url("../../image/homepage/sidepage.png");

}
/*slide*/
.swipe {
	overflow: hidden;
	visibility: hidden;
	position: relative;
}
.swipe-wrap {
	overflow: hidden;
	position: relative;
}
.swipe-wrap > div {
	float: left;
	width: 100%;
	position: relative;
}

.swipe-wrap > div {
	height: 200px;
	overflow: hidden;
	position: relative;
}
/*图片的大小*/
.swipe-wrap img {
	height: 200px;
	width: 100%;
}
/*图片的轮番*/
#slide {
	height: 200px;
	margin: 0 auto;
	position: relative;
}
/*字体颜色*/

#pointer {
	position: absolute;
	right: 10px;
	bottom: 12px;
	overflow: hidden;
}
#pointer li {
	float: left;
	list-style: none;
}
#pointer li a {
	display: inline-block;
	width: 6px;
	height: 6px;
	border-radius: 6px;
	border: 1px solid #55abce;
	margin-right: 4px;
}
#pointer li a.active {
	background-color: #55abce;
}

/*下面的样式 */
#activity {
	background-color: #f2f2f2;
}


#activity .item {
	padding-top: 10px;
    -webkit-box-shadow: 0px 4px 4px #dddcdc;
	box-shadow: 0px 4px 4px #dddcdc;
	margin-bottom:10px;
}
#activity .pic {
	height: 160px;
	width: 40%;
	margin: 0 auto 20px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
	border: 1px solid #0062CC;
	background-repeat: no-repeat;
	background-position: center top;
	-webkit-background-size: cover;
	background-size: cover;
	margin-left: 10px;
	display: inline-block;
	float: left;
}
#activity h2 {
	font-size: 16px;
	padding-left: 10em;
	text-align: center;
	padding-top:5px;
	padding-bottom: .8em;
}
#activity p {
  
    color: #999;
 	font-size: 13px;
	padding-left: 13em;
	padding-bottom: .8em;
	
}

#activity .item a {
	display: block;
}
/*下面的样式 */
